<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Map_act"
    android:background= "@drawable/map_back">

    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="203dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/nulls" />

    <ImageButton
        android:id="@+id/hosp"
        android:layout_width="55dp"
        android:layout_height="52dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/rest"
        android:layout_marginStart="53dp"
        app:srcCompat="@drawable/hosp" />

    <ImageButton
        android:id="@+id/rest"
        android:layout_width="59dp"
        android:layout_height="52dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="45dp"
        android:layout_marginTop="39dp"
        app:srcCompat="@drawable/rest" />

    <ImageButton
        android:id="@+id/Show"
        android:layout_width="64dp"
        android:layout_height="54dp"
        android:layout_alignTop="@+id/hosp"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/supermarket" />

    <ImageButton
        android:id="@+id/ser"
        android:layout_width="56dp"
        android:layout_height="52dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="152dp"
        android:layout_toEndOf="@+id/hosp"
        app:srcCompat="@drawable/search" />

    <ImageButton
        android:id="@+id/mic"
        android:layout_width="55dp"
        android:layout_height="52dp"
        android:layout_alignTop="@+id/ser"
        android:layout_toStartOf="@+id/rest"
        app:srcCompat="@drawable/voice" />



</RelativeLayout>